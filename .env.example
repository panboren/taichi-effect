# 应用标题
VITE_APP_TITLE=MyAdmin管理系统

# 基础路径
VITE_BASE_PATH=/

# 端口
VITE_PORT=5173

# 是否自动打开浏览器
VITE_OPEN=true

# API 地址
VITE_API_URL=http://localhost:3000

# 输出目录
VITE_OUT_DIR=dist

# 是否生成 sourceMap（生产环境设为 false）
VITE_SOURCEMAP=false

# 是否移除 console（生产环境设为 true）
VITE_DROP_CONSOLE=false

# 是否移除 debugger
VITE_DROP_DEBUGGER=false

# 是否开启租户模式
VITE_APP_TENANT_ENABLE=false

# 是否开启验证码
VITE_APP_CAPTCHA_ENABLE=true

# 是否开启请求加密
VITE_APP_API_ENCRYPT_ENABLE=false

# 加密密钥（生产环境请修改为强密钥）
VITE_APP_CRYPTO_KEY=my-secret-key-change-in-production

# CDN 配置（可选）
VITE_CDN_URL=https://cdn.example.com
