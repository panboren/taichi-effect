# 测试环境配置

# 应用标题
VITE_APP_TITLE=MyAdmin管理系统(测试环境)

# API 地址
VITE_API_URL=https://test-api.example.com

# 构建输出目录
VITE_OUT_DIR=dist

# 是否自动打开浏览器
VITE_OPEN=false

# 开发服务器端口
VITE_PORT=5173

# 是否启用 sourcemap
VITE_SOURCEMAP=true

# 生产环境是否移除 console
VITE_DROP_CONSOLE=false

# 生产环境是否移除 debugger
VITE_DROP_DEBUGGER=false

# 是否启用租户功能
VITE_APP_TENANT_ENABLE=false

# 是否启用验证码
VITE_APP_CAPTCHA_ENABLE=false

# 是否启用 API 加密
VITE_APP_API_ENCRYPT_ENABLE=false
