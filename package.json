{
  "name": "my-admin-framework",
  "private": true,
  "version": "1.0.0",
  "description": "现代化企业级管理系统框架",
  "type": "module",
  "scripts": {
    "mock": "node mock-server.js",
    "dev": "vite --mode dev",
    "dev:mock": "concurrently \"npm run mock\" \"npm run dev\"",
    "build": "vue-tsc && vite build",
    "build:dev": "vue-tsc && vite build --mode dev",
    "build:prod": "vue-tsc && vite build --mode prod",
    "build:test": "vue-tsc && vite build --mode test",
    "build:staging": "vue-tsc && vite build --mode staging",
    "build:analyze": "vue-tsc && vite build --mode prod && npm run analyze",
    "analyze": "rollup-plugin-visualizer -s dist/stats.html",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,vue,css,scss,html,json,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,vue,css,scss,html,json,md}",
    "type-check": "vue-tsc --noEmit",
    "audit": "npm audit",
    "audit:fix": "npm audit fix"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/core": "^12.2.0",
    "@wangeditor/editor": "^5.1.23",
    "axios": "^1.7.9",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "element-plus": "^2.9.1",
    "jsencrypt": "^3.3.2",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.7.1",
    "taichi.js": "^0.0.36",
    "vue": "^3.5.13",
    "vue-i18n": "^11.0.1",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node22": "^22.0.0",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.2",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.31.0",
    "express": "^4.21.2",
    "imagemin": "^9.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^11.0.1",
    "imagemin-webp": "^8.0.0",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.83.4",
    "typescript": "~5.6.3",
    "unocss": "^0.65.0",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-vue-components": "^0.28.0",
    "vite": "^6.0.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^2.1.10"
  }
}
